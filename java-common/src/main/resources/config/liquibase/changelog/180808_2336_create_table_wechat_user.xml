<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    
    <changeSet id="180808_2336_create_table_wechat_user" author="cinling">
        <createTable tableName="wechat_user">
            <column name="id" type="int unsigned" autoIncrement="true" remarks="">
                <constraints primaryKey="true" nullable="false" />
            </column>
            <column name="openid" type="char(64)" remarks="用户唯一标识">
                <constraints nullable="false" />
            </column>
            <column name="session_key" type="char(128)" remarks="会话密钥">
                <constraints nullable="false" />
            </column>
            <column name="update_time" type="int unsigned" remarks="更新时间">
                <constraints nullable="false" />
            </column>
            <column name="create_time" type="int unsigned" remarks="创建时间">
                <constraints nullable="false" />
            </column>
        </createTable>

        <modifySql dbms="mysql">
            <append value="ENGINE=INNODB CHARSET=utf8mb4 COLLATE utf8mb4_unicode_ci"/>
        </modifySql>
    </changeSet>
    
</databaseChangeLog>